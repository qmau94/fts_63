<div class="field td-q-search">
  <div class="col-md-3 height">
    <%= f.attribute_fields do |a| %>
      <%= a.attribute_select associations: [:question] %>
    <% end %>
  </div>
  <div class="col-md-3 height">
    <%= f.predicate_select %>
  </div>
  <div class="col-md-3 height">
    <%= f.value_fields do |v| %>
      <%= v.text_field :value %>
    <% end %>
  </div>
  <%= link_to t("users.remove"), "#",
    class: "remove_fields btn btn-danger hvr-pulse-shrink" %>
</div>
